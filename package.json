{
  "name": "cloudfare-hono-waitlist-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev:client": "bun run --bun astro dev",
    "build:client": "bun run --bun astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint 'src/**/*.{js,ts,tsx,astro}'",
    "dev:server": "wrangler dev",
    "dev": "concurrently \"bun run dev:client\" \"bun run dev:server\"  ",
    "dryrun:staging": "bun run build:client && wrangler deploy --dry-run --env staging",
    "dryrun:prod": "bun run build:client && wrangler deploy --dry-run --env production",
    "deploy:staging": "bun run build:client && wrangler deploy --env staging",
    "deploy:prod": "bun run build:client && wrangler deploy --env production"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.6",
    "concurrently": "^9.2.1",
    "hono": "^4.11.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "wrangler": "^4.56.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2"
  }
}
